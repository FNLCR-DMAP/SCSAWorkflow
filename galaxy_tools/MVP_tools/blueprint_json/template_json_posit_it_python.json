  "contents": {
    "type": "code",
    "code": {
      "codeLanguage": "PYTHON",
      "codeTemplate": "@synchronous_node_execution\ndef Load_CSV():\n\n    import matplotlib.pyplot as plt\n    # text params\n\n    text = \"{{{Label}}}\"\n    text_color = '{{{Label_font_color}}}'\n    text_size = {{{Label_font_size}}}\n    text_fontface = \"{{{Label_font_type}}}\"\n    text_fontfamily = '{{{Label_font_family}}}'\n    bold = {{{Label_Bold}}}\n\n # background params\n    \n    fill_color = '{{{Background_fill_color}}}'\n    fill_alpha = {{{Background_fill_opacity}}}\n    border_alpha = 1\n    border_width = 0\n\n# image params\n    image_width = {{{Page_width}}}\n    image_height = {{{Page_height}}}\n    image_resolution = {{{Page_DPI}}}  \n\n# output value\n    tag = \"CCBR\"\n\n    # RUN ====   \n\n# paints    \n    paints = {\n        'White': '#FFFFFF',\n        'LightGrey': '#D3D3D3',\n        'Grey': '#999999',\n        'Black': '#000000',\n        'Red1': '#F44E3B',\n        'Red2': '#D33115',\n        'Red3': '#9F0500',\n        'Orange1': '#FE9200',\n        'Orange2': '#E27300',\n        'Orange3': '#C45100',\n        'Yellow1': '#FCDC00',\n        'Yellow2': '#FCC400',\n        'Yellow3': '#FB9E00',\n        'YellowGreen1': '#DBDF00',\n        'YellowGreen2': '#B0BC00',\n        'Yellowgreen3': '#808900',\n        'Green1': '#A4DD00',\n        'Green2': '#68BC00',\n        'Green3': '#194D33',\n        'Teal1': '#68CCCA',\n        'Teal2': '#16A5A5',\n        'Teal3': '#0C797D',\n        'Blue1': '#73D8FF',\n        'Blue2': '#009CE0',\n        'Blue3': '#0062B1',\n        'Purple1': '#AEA1FF',\n        'Purple2': '#7B64FF',\n        'Purple3': '#653294',\n        'Magenta1': '#FDA1FF',\n        'Magenta2': '#FA28FF',\n        'Magenta3': '#AB149E'\n    }   \n\n#image: png\n    fig = plt.figure(figsize=(image_width, image_height), dpi=image_resolution)\n    fig.patch.set_facecolor(paints[fill_color])\n    fig.patch.set_alpha(fill_alpha/100)\n    for ax in fig.get_axes():\n        for item in ([ax.title, ax.xaxis.label, ax.yaxis.label] +\n                    ax.get_xticklabels() + ax.get_yticklabels()):\n            item.set_fontsize(text_size)\n            item.set_fontfamily(text_fontfamily)\n            item.set_fontstyle(text_fontface)\n            if bold:\n                item.set_fontweight('bold')\n\n    #plt.set_facecolor(paints[fill_color]+hex_fill)    \n\n    fig.text(0.5,0.5,text, fontsize=text_size, color=paints[text_color], ha='center', va='center')\n    fig.savefig('graphicsFile.png', format='png', transparent=True, bbox_inches='tight')\n    plt.show()\n    \n    return(None)",
      "shouldPersist": false
    }
